{
  "name": "educationalplatform",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha ./test/test.js --timeout 10000 --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coveralls": "npm run coverage  && cat ./coverage/lcov.info | coveralls"
  },
  "nyc": {
    "_comment": "Purpose of this part: to do code coverage within Mocha/Chai using Istanbul",
    "register": "babel-register",
    "include": [
      "**/*.js",
      "./*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/coverage/**"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awamry/EducationalPlatform.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/awamry/EducationalPlatform/issues"
  },
  "homepage": "https://github.com/awamry/EducationalPlatform#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "expect": "^24.7.1",
    "express": "^4.16.4",
    "gulp": "^3.9.1",
    "gulp-bg": "0.0.8",
    "gulp-istanbul": "^1.1.3",
    "gulp-mocha": "^2.2.0",
    "gulp-shell": "^0.7.0",
    "mocha": "^6.1.4",
    "request": "^2.88.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "mocha-lcov-reporter": "^1.3.0"
  }
}